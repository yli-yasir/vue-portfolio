<%# --DOCUMENTATION 
    This function must be called on objects which define the fields in
    the project model schema:

    Simplified view on the required fields 
    description : String 
    youtubeEmbed : String
    imgUrls : [String]
    links: [{ label: String, url: String }]
%>

function(project){

    var carouselHtml = `<%- include('../templates/components/carousel/container.ejs') %>`;
    $('.true-content').append(carouselHtml);

    $.each(project.imgUrls,function(index,url){
        $('.carousel-inner').append(`<%- include('../templates/components/carousel/item.ejs') %>`)
    });

}