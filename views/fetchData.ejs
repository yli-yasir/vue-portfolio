<script>
    $.getJSON("<%- from %>",function(data){
      $.each(data,function(index,value){
        thumbnailUrl = value.imgUrl;
        name = value.name;
        shortDescription = value.shortDescription;
        moreDetailsUrl = "<%- currentPath %>" + "/" + value._id;
        cardHTML = `<%- include("card.ejs") %>`
        $('.content').append(cardHTML);
      })
    })
</script>