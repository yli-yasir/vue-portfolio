<%- include('parts/header.ejs',{activePage: "home"}) %>

<div class="content">
  <div class="aspect-ratio-16by9">
    <div
      id="carouselExampleControls"
      class="carousel slide aspect-ratio-child"
      data-ride="carousel"
    >
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="https://i.imgur.com/SB2ce66.jpg" alt="First slide" />
        </div>
        <div class="carousel-item">
          <img src="https://i.imgur.com/uzQzjK8.png" alt="Second slide" />
        </div>
        <div class="carousel-item">
          <iframe
            src="https://www.youtube.com/embed/jw2etItmQjk?list=RDjw2etItmQjk"
            frameborder="0"
            allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
            allowfullscreen
          ></iframe>
        </div>
      </div>
      
      <a
        class="carousel-control-prev"
        href="#carouselExampleControls"
        role="button"
        data-slide="prev"
      >
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a
        class="carousel-control-next"
        href="#carouselExampleControls"
        role="button"
        data-slide="next"
      >
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
</div>

<%- include('parts/footer.ejs') %>
